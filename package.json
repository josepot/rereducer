{
  "name": "rereducer",
  "version": "v3.0.0-alpha.1",
  "license": "MIT",
  "repository": "github:josepot/rereducer",
  "bugs": "https://github.com/josepot/rereducer/issues",
  "keywords": [
    "redux",
    "reducer",
    "enhancers",
    "boilerplate",
    "functional",
    "declarative"
  ],
  "author": "Josep M Sobrepere (https://github.com/josepot)",
  "main": "lib/rereducer.js",
  "types": "types/index.d.ts",
  "unpkg": "dist/rereducer.js",
  "module": "es/rereducer.js",
  "files": [
    "dist",
    "lib",
    "es",
    "src",
    "types/index.d.ts"
  ],
  "scripts": {
    "clean": "rimraf lib dist es coverage",
    "format": "prettier --write \"{src,test}/**/*.js\"",
    "format:check": "prettier --list-different \"{src,test}/**/*.js\"",
    "lint": "eslint src test",
    "pretest": "npm run build:commonjs",
    "test": "cross-env BABEL_ENV=commonjs jest",
    "test:watch": "npm test -- --watch",
    "test:cov": "npm test -- --coverage",
    "test:types": "dtslint types",
    "build:commonjs": "cross-env NODE_ENV=cjs rollup -c -o lib/rereducer.js",
    "build:es": "cross-env BABEL_ENV=es NODE_ENV=es rollup -c -o es/rereducer.js",
    "build:umd": "cross-env BABEL_ENV=es NODE_ENV=development rollup -c -o dist/rereducer.js",
    "build:umd:min": "cross-env BABEL_ENV=es NODE_ENV=production rollup -c -o dist/rereducer.min.js",
    "build": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "prepare": "npm run clean && npm run format:check && npm run lint && npm test && npm run build"
  },
  "dependencies": {
    "loose-envify": "^1.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/node": "^7.0.0",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.6.0",
    "cross-env": "^5.1.4",
    "dtslint": "^0.3.0",
    "eslint": "^5.6.1",
    "eslint-config-react-app": "^3.0.3",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.7.0",
    "glob": "^7.1.1",
    "jest": "^23.6.0",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
    "rollup": "^0.66.4",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-terser": "^3.0.0"
  },
  "npmName": "rereducer",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ],
  "browserify": {
    "transform": [
      "loose-envify"
    ]
  },
  "jest": {
    "testRegex": "(/test/.*\\.spec.js)$",
    "testURL": "http://localhost/"
  },
  "sideEffects": false
}

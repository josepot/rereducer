{
  "name": "rereducer",
  "version": "2.0.0",
  "main": "./lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "scripts": {
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es",
    "build:umd": "cross-env BABEL_ENV=commonjs NODE_ENV=development webpack src/index.js dist/rereducer.js",
    "build:umd:min": "cross-env BABEL_ENV=commonjs NODE_ENV=production webpack src/index.js dist/rereducer.min.js",
    "build": "npm run build:commonjs && npm run build:es && npm run build:umd && npm run build:umd:min",
    "clean": "rimraf lib dist es",
    "lint": "eslint src",
    "prepublish": "npm run clean && npm run build",
    "test": "cross-env BABEL_ENV=commonjs NODE_ENV=test mocha --compilers js:babel-register --recursive",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/josepot/rereducer.git"
  },
  "files": [
    "dist",
    "lib",
    "src",
    "es"
  ],
  "keywords": [
    "redux",
    "reducer",
    "boilerplate",
    "declarative",
    "watchers"
  ],
  "author": "Josep M Sobrepere (https://github.com/josepot)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/josepot/rereducer/issues"
  },
  "homepage": "https://github.com/josepot/rereducer",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-exponentiation-operator": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-runtime": "^6.23.0",
    "cross-env": "^3.2.3",
    "eslint": "^3.17.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "expect": "^1.20.2",
    "mocha": "^3.2.0",
    "rimraf": "^2.6.1",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "invariant": "^2.2.2"
  }
}
